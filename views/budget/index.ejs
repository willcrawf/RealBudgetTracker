<%- include('../partials/header') %>
<table id='list' class = "table">
    <thead>
      <tr>
        <th>Item Name</th>
        <th>Price</th>
        <th>Location</th>
        <th>Date</th>
        <th>Category</th>
        <th>Upcoming</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% items.forEach((m) => { %>
        <tr>
          <td><%= m.name %></td>
          <td><%= m.cost %></td>
          <td><%= m.location %></td>
          <td><%= m.date %></td>
          <td><%= m.category %></td>
          <% if (m.upcoming) { %>
              <%= m.upcoming = "UPCOMING" %>
              <td><%= m.upcoming %></td>
          <% } else { %>
             <td><%= m.upcoming = "No" %></td>
            <% } %> 
        </tr>
      <% }) %>
    </tbody>
  </table>

<%- include('../partials/footer') %>
